@using Microsoft.AspNetCore.Routing

@{var culture = ((string)Context.GetRouteData().Values["culture"])?.ToLower() ?? "en"; }

@await Html.PartialAsync("Help/algos/equihash-" + culture, Model, new ViewDataDictionary(ViewData)
{
    { "OfficialWalletUrl", "https://z.cash/wallets/" },
    { "WalletRegex", "^(t|z).+" },
})
