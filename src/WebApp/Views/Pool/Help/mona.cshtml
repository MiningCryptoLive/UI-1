@using Microsoft.AspNetCore.Routing

@{var culture = ((string)Context.GetRouteData().Values["culture"])?.ToLower() ?? "en"; }

@await Html.PartialAsync("Help/algos/cryptodredge-" + culture, Model, new ViewDataDictionary(ViewData)
{
    { "OfficialWalletUrl", "https://monacoin.org/" },
    { "CryptoDredgeAlgo", "lyra2v2" },
    { "WalletRegex", "^M.+" },
})
